package AbenesDesamitoGabriel;
public class StudentInformation {
    
    String university;
    String college;
    String term;
    String SY;
    
    private String idNumber;
    private String familyName;
    private String firstName;
    private String academicProgram;
    private String track;
    
    StudentInformation (String term, String college, String SY) {
    this.university = "University of the Cordilleras";
    this.college = college;
    this.term = term;
    this.SY = SY;
    }    

    
    public void setIdNumber(String idNumber) {
        this.idNumber = idNumber;
    }
    
    public String getIdNumber() {
        return idNumber;
    }
        //IdNumber
    
    public void setFamilyName(String familyName) {
        this.familyName = familyName;
    }
    
    public String getFamilyName() {
        return familyName;
    }
        //FamilyName
    
    public void setFirstName(String firstName) {
        this.firstName = firstName;
    }
    
    public String getFirstName() {
        return firstName;
    }
        //FirstName
    
    public void setAcademicProgram(String academicProgram) {
        this.academicProgram = academicProgram;
    }
    
    public String getAcademicProgram() {
        return academicProgram;
    }
        //AcademicProgram
 
    public void setTrack(String track) {
        this.track = track;
    }
    
    public String getTrack() {
        return track;
    }    
        //Track
    
    public void prints() {
        System.out.println(this.university);
        System.out.println(this.college);
        System.out.println(this.term + " " + this.SY);        
        System.out.println("");                
        System.out.println("Student Information: \n" + "ID Number: " + this.idNumber + "\n" + "Name: " + this.familyName + this.firstName + "\n" + "Academic Program: " + this.academicProgram + this.track);
        System.out.println("");
        System.out.println("****** ");
    } // End of prints
    
    
} //End of class
